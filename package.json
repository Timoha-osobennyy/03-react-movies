{
  "name": "03-react-movies",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "homepage": "https://timoha-osobennyy.github.io/03-react-movies/",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite --open /index.html",
    "dev:local": "vite --mode development --open /index.html",
    "build": "vite build --mode production",
    "build:gh-pages": "vite build --base=/03-react-movies/ --mode production",
    "preview": "vite preview --open /index.html",
    "deploy": "npm run build:gh-pages && gh-pages -d dist -b gh-pages",
    "deploy:vercel": "npm run build && vercel --prod",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "postinstall": "vite build"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "modern-normalize": "^3.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2"
  },
  "devDependencies": {
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.7.0",
    "gh-pages": "^6.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.5"
  }
}
